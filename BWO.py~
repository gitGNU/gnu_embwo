try:
   import os
   import sys
   import paths
   import ui
   from PyQt5.QtCore import *
   from PyQt5.QtWidgets import *

except ImportError as err:
   print("couldn't load module. %s" % (err))
   sys.exit(2)

class BWO(object):

 def __init__(self):
   print('initialized BWO');
   print('version: 0.00');
   self.app = QApplication(sys.argv)

   self.screen = ui.Form()

 def run(self):
   self.screen.show()

 def exit(self,application):
   sys.exit(application.exec_())


app = BWO()

app.run()

app.exit(app)

